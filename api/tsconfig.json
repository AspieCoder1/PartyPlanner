{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true  /* Disallow inconsistently-cased references to the same file. */,
    "outDir": "dist",
    "skipLibCheck": true,
  },
  "exclude": ["node_modules"],
  "include": ["src"],
}
